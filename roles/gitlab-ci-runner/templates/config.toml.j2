concurrent = {{ gitlab_runner_concurrent }}
check_interval = 0

[[runners]]
  name = "{{ gitlab_runner_name }}"
  url = "{{ gitlab_url }}"
  token = "{{ gitlab_token }}"
  executor = "docker"
  [runners.docker]
    tls_verify = false
    image = "{{ gitlab_runner_image }}"
    privileged = false
    disable_cache = false
    volumes = ["/cache"]
    shm_size = 0
  [runners.cache]
